{
  "configurations": [
    {
      "name": "macos-clang-arm64",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "__DEBUG__",
        "__DEBUGLOG__"
      ],
      "windowsSdkVersion": "10.0.22000.0",
      "compilerPath": "/usr/bin/clang",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "macos-clang-arm64",
      "configurationProvider": "ms-vscode.makefile-tools"
    }
  ],
  "version": 4
}